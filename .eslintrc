{
    "rules": {
        "indent": [2,"tab", {"SwitchCase":1}],
        "quotes": [2,"single"],
        "semi": [2,"never"],
        "no-unused-vars": ["warn", { "vars": "all", "args": "none" }],
        "no-console": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "node" : true,
        "jquery": true,
        "mocha" :true
    },
    "extends": "eslint:recommended",
     "parserOptions": {
        "sourceType": "module"
    },
    "globals":{
        "axios" :true
    }
}
